{
    "version": "2018-05-29",
    "statements": [
        "SELECT ParkingSpotID,ActualEntryTime, ActualExitTime,BasicCost,ParkingFloorID,WingCode,ParkingLetter,Latitude,Longitude FROM mysqlauroradb.ParkingSpotReservation as t1 INNER JOIN mysqlauroradb.ParkingTransactionDetails as t2 ON t1.ID =t2.ParkingReservationID INNER JOIN mysqlauroradb.ParkingSpotDetails as t3 ON t3.ID = t1.ParkingSpotID WHERE t1.UserID = $ctx.args.userId"
    ]
}
